{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/user/src",
  "monorepo": true,
  "root": "apps/user",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/user/tsconfig.app.json"
  },
  "projects": {
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    },
    "order": {
      "type": "application",
      "root": "apps/order",
      "entryFile": "main",
      "sourceRoot": "apps/order/src",
      "compilerOptions": {
        "tsConfigPath": "apps/order/tsconfig.app.json"
      }
    },
    "grpc": {
      "type": "library",
      "root": "libs/grpc",
      "entryFile": "index",
      "sourceRoot": "libs/grpc/src",
      "compilerOptions": {
        "tsConfigPath": "libs/grpc/tsconfig.lib.json"
      }
    }
  }
}